#include <stdio.h>
int main()
{
	int n,i;
	float num[100],sum=0.0,avg;
	
	printf("Enter the numbers of elements:");
	scanf("%d",&n);
	
	for(i=1;i<=n;i++)
	{
		printf("%d Enter the number:",i);
		scanf("%f",&num[i]);
		sum = sum + num[i]; 
    }
    
    avg=sum/n;
    printf("Average=% 2f",avg);
    return 0;
}
